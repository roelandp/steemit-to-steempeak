{
  "name": "Steemit to SteemPeak",
  "description": "Redirect Steemit.com to SteemPeak.com",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://steemit.com/*",
    "*://www.steemit.com/*",
    "webRequestBlocking"
  ]
}
